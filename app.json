{
  "expo": {
    "name": "Gossip",
    "slug": "gossip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "scheme": "gossip",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bitcode": false,
      "bundleIdentifier": "com.ssabee1722.gossip",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "voip",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "package": "com.ssabee1722.gossip",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/app-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-web-browser",
      "expo-asset",
      "expo-font",
      "@react-native-community/datetimepicker",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.gossip",
          "enableGooglePay": true
        }
      ],
      "expo-audio",
      "expo-localization",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Gossip to access your photos to share with friends.",
          "cameraPermission": "Allow Gossip to access your camera to take and send photos."
        }
      ],
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow Gossip to access your camera to make video calls.",
          "microphonePermission": "Allow Gossip to access your microphone to make audio and video calls."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f391820-3361-4bbe-86ed-87f142749d65"
      }
    }
  }
}